set name w2a

set t $time
set rbox [lindex $data 0]
set att  [lindex $data 1]
set tr   [lindex [graphene_get_prev ${name}_meas:3 $t] 1]
set time $t

if {$tr == 1 && $att == 20} {
  if {$time<1675209600} { # 2023-02-01
    # measured 2023-01-20
    set data "-38120000.789546 3049257.019724 -2496224.547432 51860.934162 -25.179435 749.072651 -793.152549 2647.740035 -115.376234 76.803138 1.944127 6.547143"
    return 1
  }
  if {$time < 1694559600} { # 2023-09-13
    # measured 2023-02-24, changed before 2023-01-20
    set data "9795.975244 -1266.726728 33.493013 -57.879193 3.250716 13.598215
              24.594212 -80.132877 -1.073777 -60.683358 0.813691 1.783657"
    return 1
  }
  # measured 2023-09-23
  set data "7151.470988 -1041.120190 166.490627 -47.639830 2.987345 10.947746
            103.953464 -340.927968 -0.243455 -63.371343 1.185177 2.867574"
  return 1
}

return 0


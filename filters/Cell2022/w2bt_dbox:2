set name w2bt

set t $time
set rbox [lindex $data 0]
set att  [lindex $data 1]
set tr   [lindex [graphene_get_prev ${name}_meas:3 $t] 1]
set time $t

if {$tr == 1 && $att == 20} {
  if {$time < 1682417940} {
    # measured 2023-01-20
    set data "5981.960295 -9255.741833 1581.078235 -54.437167 12.129090 -0.000042
              -2795.741396 2833.515181 1587.603689 -151.905442 12.139387 0.000012"
    return 1
  }
  if {$time < 1694559600} {
    # measured 2023-05-02
    # after switching ch8 -> ch14
    set data "-4301.711954 6188.550935 466.592792 -80.399469 11.977707 -0.000624
               3107.800902 -3420.539773 -1429.288644 149.274662 11.522082 -0.001331"
    return 1
  }
  # measured 2023-09-23
  set data "204.792201 -775.011827 1224.384185 -60.904057 12.727031 -2.886490
            271.715275 -798.635009 33.064128 -5.058200 11.676593 1.060623"
  return 1
}

return 0


set v [lindex $data 0]
if {$v<=2.97} {set v [expr {-3.2106 + 2.75762*$v - 2.09382*$v**2\
                            + 1.00697*$v**3 - 0.24882*$v**4 + 0.02383*$v**5}]}\
else {set v [expr {0.08657 - 3.04116*$v + 2.10531*$v**2 - 0.6334*$v**3\
                 + 0.09888*$v**4 - 0.00774*$v**5 + 0.000240649*$v**6}]}
set data [expr 10**$v]
